#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int n;
    scanf("%d",&n);
    int i=n,j;
    if(i/10==0)
    {
        j=n*n;
        if(j%10==n)
            printf("Automorphic Number");
        else
            printf("Not Automorphic Number");
    }
    else
        printf("Invalid Input");
    
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    return 0;
}
