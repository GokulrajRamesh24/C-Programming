#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int m;
    scanf("%d",&m);
    (m%2==0)?printf("The given input %d is Even",m):printf("The given input %d is Odd",m);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
