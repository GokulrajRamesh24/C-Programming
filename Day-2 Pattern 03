#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int n,i,j,m;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        if(i==0 || i==n-1)
        {
            for(m=0;m<n;m++)
            {
                printf("* ");
            }
        }
        else
        {
            for(j=0;j<n;j++)
            {
                if(j==0 || j==n-1)
                    printf("* ");
                else
                    printf("  ");   
            }
            printf(" ");
        }
        printf("\n");
        }
    
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    return 0;
}
