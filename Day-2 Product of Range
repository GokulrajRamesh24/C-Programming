#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,i,k=1;
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        k*=i;
    }
    printf("Product of value is %d",k);
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
