#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    if(n>0)
    {
        printf("The Given Number %d is Positive.",n);
    }
    else if(n<0)
    {
        printf("The Given Number %d is Negative.",n);
    }
    else
    {
        printf("The Given Number %d is Zero.",n);
    }

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
