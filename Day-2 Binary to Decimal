#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    int n,i,j,sum=0,q,t=0;
    scanf("%d",&n);
    while(n>0)
    {
        q=n%10;
        if(q==1)
        {
            sum+=pow(2,t);
        }
        n=n/10;
        t++;
    }
    printf("%d",sum);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
