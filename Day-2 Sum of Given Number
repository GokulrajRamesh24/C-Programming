#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int i=0;
    do
    {
        int n;
        scanf("%d",&n);
        i+=n;
        if(n==-1)
        {
            ++i;
            break;
        }     
    }while(1);
    printf("%d",i);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    return 0;
}
