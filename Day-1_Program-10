//Type Casting
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a;
    char b;
    scanf("%d\n%c",&a,&b);
    printf("Integer of %d is %c\n",a,a);
    printf("Character of %c is %d",b,b);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
