#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int n,i;
    scanf("%d",&n);
    int a=6,b=3;;
    while(n>0)
    {
        printf("%d ",a);
        a+=b;
        b+=2;
        n--;
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    return 0;
}
