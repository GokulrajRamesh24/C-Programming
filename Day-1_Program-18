//Harry Potter and the Sorcerer's Stone
#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    unsigned int a;
    scanf("%d",&a);
    int b,c=0;
    for(int i=0;i<4;i++)
    {
        b=a%10;
        if(i==0 || i==3)
        {
          c+=b;  
        }
        a/=10;   
    }
    printf("%d",c);
    
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    return 0;
}
