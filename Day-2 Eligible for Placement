#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char a[100];
    int b,c;
    scanf("%s\n%d%d",a,&b,&c);
    if((b==1 && c>70))
    {
        printf("Name of the Student:%s",a);
        printf("\n%s is Eligible for Placement",a);
    }
    else if((b==1 || b==2) && c>75)
    {
        printf("Name of the Student:%s",a);
        printf("\n%s is Eligible for Placement",a);
    }
       
    else
    {
        printf("Name of the Student:%s",a);
        printf("\n%s is Not Eligible for Placement",a);
    }

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
