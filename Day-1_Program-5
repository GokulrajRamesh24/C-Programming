//Decimal Formatting 02
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    float a,b;
    scanf("%f",&a);
    b=floor(a*100)/100;
    printf("%.2f\n",b);
    printf("%.2f\n",a);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
