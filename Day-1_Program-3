//Statement Execution
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a;
    float b;
    char c,d[100],e[100];
    scanf("%d%f\n%c%s\n%[^\n]s",&a,&b,&c,d,e);
    printf("Integer is %d\n",a);
    printf("Float is %.2f\n",b);
    printf("Character is %c\n",c);
    printf("Strings are %s and %s",d,e);


    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
